<?xml version="1.0" encoding="UTF-8"?>
<module
	xmlns="http://www.varylab.com/module"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
	<manifest>
		<id>hf654fsknbgt5675</id>
		<name>Spring Energy Optimizer</name>
		<revision>1</revision>
		<category>Optimization</category>
	</manifest>
	<description>
		The spring energy optimizer is a collection of energies. You can 
		have springs of constant, mean, or zero length.
		It also contains optimizers for Dirichlet energy minimization, 
		edge length equalization, n-gon regularization, and spherical 
		point distribution.
	</description>
	<plugin class="de.varylab.varylab.plugin.meshoptimizer.SpringOptimizer"/>
	<plugin class="de.varylab.varylab.plugin.meshoptimizer.DirichletEnergyOptimizer"/>
	<plugin class="de.varylab.varylab.plugin.meshoptimizer.EdgeLengthEqualizerOptimizer"/>
	<plugin class="de.varylab.varylab.plugin.meshoptimizer.ElectrostaticSphereOptimizer"/>
	<plugin class="de.varylab.varylab.plugin.meshoptimizer.NgonRegularizer"/>
</module>